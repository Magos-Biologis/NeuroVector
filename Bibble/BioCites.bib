@Article{Valle2022,
  author       = {Valle, Jemy A. Mandujano and Madureira, Alexandre L.},
  journal      = {Neural Computation},
  title        = {Parameter Identification Problem in the Hodgkin-Huxley Model},
  year         = {2022},
  issn         = {1530-888X},
  month        = mar,
  number       = {4},
  pages        = {939--970},
  volume       = {34},
  abstract     = {The Hodgkin and Huxley (H-H) model is a nonlinear system of four equations that describes how action potentials in neurons are initiated and propagated, and represents a major advance in the understanding of nerve cells. However, some of the parameters are obtained through a tedious combination of experiments and data tuning. In this paper, we propose the use of an iterative method (Landweber iteration) to estimate some of the parameters in the H-H model, given the membrane electric potential. We provide numerical results showing that the method is able to capture the correct parameters using the measured voltage as data, even in the presence of noise.},
  copyright    = {arXiv.org perpetual, non-exclusive license},
  date         = {2022},
  doi          = {10.1162/neco_a_01487},
  eprint       = {1903.10286},
  eprintclass  = {math.NA},
  eprinttype   = {arXiv},
  journaltitle = {Neural Computation},
  keywords     = {Numerical Analysis (math.NA), FOS: Mathematics, 92{C}20, 49{N}45},
  publisher    = {MIT Press - Journals},
}



@Article{SZCZUPAK201699,
  author       = {Lidia Szczupak},
  date         = {2016},
  journaltitle = {Current Opinion in Neurobiology},
  title        = {Functional contributions of electrical synapses in sensory and motor networks},
  doi          = {10.1016/j.conb.2016.08.005},
  issn         = {0959-4388},
  note         = {Microcircuit computation and evolution},
  pages        = {99--105},
  url          = {https://www.sciencedirect.com/science/article/pii/S0959438816301179},
  volume       = {41},
  abstract     = {Intercellular interactions in the nervous system are mediated by two types of dedicated structural arrangements: electrical and chemical synapses. Several characteristics distinguish these two mechanisms of communication, such as speed, reliability and the fact that electrical synapses are, potentially, bidirectional. Given these properties, electrical synapses can subserve, in addition to synchrony, three main interrelated network functions: signal amplification, noise reduction and/or coincidence detection. Specific network motifs in sensory and motor systems of invertebrates and vertebrates illustrate how signal transmission through electrical junctions contributes to a complex processing of information.},
